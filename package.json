{
  "name": "simple-rabbitmq-project",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:start": "node dist/src/index.js",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "jest": "^26.6.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "amqplib": "^0.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.4"
  }
}
